{
  "name": "administrat_app",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "front-end",
    "back-end",
    "electron"
  ],
  "scripts": {
    "start:front": "npm run start --workspace front-end",
    "start:back": "npm run start --workspace back-end",
    "start:electron": "npm run start --workspace electron",
    "install:all": "npm install",
    "start": "npm-run-all --parallel start:front start:back start:electron"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
